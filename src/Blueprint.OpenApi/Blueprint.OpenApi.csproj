<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net472</TargetFrameworks>
    <Title>Blueprint - OpenAPI</Title>
    <Description>Blueprint: Enables an /openapi endpoint for Blueprint API</Description>
    <PackageTags>OpenApi;Swagger;$(PackageBaseTags)</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NSwag.Core" Version="13.3.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Blueprint.Api.Http\Blueprint.Api.Http.csproj" />
  </ItemGroup>
</Project>
